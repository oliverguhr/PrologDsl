Model:
	greetings+=Greeting*;
	
Greeting:
	'Hello' name=ID '!';
	
	
Functor:ident;
Atom: ident| variable | Number;
Number:INT;
Folge: Atom (',' Atom)* ;

	 
terminal variable: 'A'..'Z';
terminal ident: 'a'..'z'('a'..'z'|'A'..'Z'|'_'|'0'..'9')*;